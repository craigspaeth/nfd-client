doctype 5
html( class=uaFamily )
  head
    title NoFeeDigs | Find no fee apartments in New York City.
    meta( name='viewport', content='width=device-width', initial-scale='1.0,user-scalable=no' )
    link( type='text/css', rel='stylesheet', href='/assets/index.css' )
    link( rel='icon', type='image/x-icon', href='/images/favicon.ico'  )
    block head
  body
    header#main-header
      a#main-header-logo( href='/' )
        | NoFeeDigs
        small Beta
      nav
        a( href='/about' ) About
        a( href='feedback' ) Give Feedback
      #main-header-social-media
        a.main-header-social-media-facebook( href='https://www.facebook.com/nofeedigs', target='_blank' )
        a.main-header-social-media-twitter( href='https://twitter.com/nofeedigs', target='_blank'  )
    include ../../feedback-modal/index
    block content
    #scripts
      != sharifyScript()
      include mixpanel.html
      include typekit.html
      script( src='http://maps.google.com/maps/api/js?sensor=true' )
      script( src='/assets/index.js' )
      script.
        mixpanel.init('!{sd.MIXPANEL_KEY}')
      block scripts